{
  "name": "spotify-now-playing-api",
  "version": "1.0.1",
  "description": "Spotify Now Playing API integration for Framer",
  "main": "index.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "deploy": "vercel --prod",
    "auth": "node scripts/get-refresh-token-simple.js",
    "auth-manual": "node scripts/manual-auth.js",
    "auth-exchange": "node scripts/exchange-token.js",
    "test": "node tests/api.test.js",
    "test:auth": "node tests/auth.test.js",
    "test:api": "node tests/api.test.js",
    "test:improved-api": "node tests/improved-api.test.js",
    "debug:vercel": "node debug/vercel-debug.js"
  },
  "keywords": [
    "spotify",
    "now-playing",
    "api",
    "framer",
    "react"
  ],
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "framer": "^2.3.0",
    "framer-motion": "^10.13.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@vercel/node": "^3.0.0",
    "node-fetch": "^2.6.12",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
